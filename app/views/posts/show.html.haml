%p#notice= notice
.container
  %p
    %b Title:
    = @post.title
  %p
    %b Body:
    = @post.body
  %p
    %b Photo:
    - @post.images.each do |p|
      = image_tag p.image_url
      = link_to "Edit Attachment", edit_image_path(p)

= link_to 'Edit', edit_post_path(@post)
\|
= link_to 'Back', posts_path
